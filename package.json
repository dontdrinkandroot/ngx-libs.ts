{
    "name": "ngx-libs.ts",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:libs": "ng build @dontdrinkandroot/ngx-extensions && ng build @dontdrinkandroot/ngx-material-extensions",
        "build:app": "ng build @dontdrinkandroot/angular-material-example",
        "build:all": "npm run build:libs && npm run build:app",
        "serve:example": "ng serve --watch --configuration development @dontdrinkandroot/angular-material-example",
        "test": "ng test",
        "test:all": "ng test @dontdrinkandroot/ngx-extensions --watch=false && ng test @dontdrinkandroot/ngx-material-extensions --watch=false && ng test @dontdrinkandroot/angular-material-example --watch=false"
    },
    "prettier": {
        "printWidth": 100,
        "singleQuote": true,
        "overrides": [
            {
                "files": "*.html",
                "options": {
                    "parser": "angular"
                }
            }
        ]
    },
    "private": true,
    "packageManager": "yarn@1.22.22",
    "dependencies": {
        "@angular/cdk": "21.0.5",
        "@angular/common": "^21.0.0",
        "@angular/compiler": "^21.0.0",
        "@angular/core": "^21.0.0",
        "@angular/forms": "^21.0.0",
        "@angular/material": "21.0.5",
        "@angular/platform-browser": "^21.0.0",
        "@angular/router": "^21.0.0",
        "@angular/service-worker": "^21.0.0",
        "@fontsource-variable/roboto-flex": "^5.2.8",
        "@fontsource/material-symbols-rounded": "^5.2.30",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@angular/build": "^21.0.4",
        "@angular/cli": "^21.0.4",
        "@angular/compiler-cli": "^21.0.0",
        "@faker-js/faker": "^10.1.0",
        "jsdom": "^27.1.0",
        "ng-packagr": "^21.0.0",
        "typescript": "~5.9.2",
        "vitest": "^4.0.8"
    }
}
